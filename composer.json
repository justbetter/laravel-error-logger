{
  "name": "justbetter/laravel-error-logger",
  "description": "Simple way to log errors with Laravel Nova support.",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0"
  },
  "authors": [
    {
      "name": "Ramon Rietdijk",
      "email": "ramon@justbetter.nl",
      "role": "Developer"
    }
  ],
  "type": "laravel-package",
  "autoload": {
    "psr-4": {
      "JustBetter\\ErrorLogger\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "JustBetter\\ErrorLogger\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "larastan/larastan": "^2.9",
    "orchestra/testbench": "^8.0|^9.0"
  }
}
