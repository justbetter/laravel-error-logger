{
    "name": "justbetter/laravel-error-logger",
    "description": "Simple way to log errors with Laravel Nova support.",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0|^12.0"
    },
    "authors": [
        {
            "name": "Ramon Rietdijk",
            "email": "ramon@justbetter.nl",
            "role": "Developer"
        }
    ],
    "type": "laravel-package",
    "autoload": {
        "psr-4": {
            "JustBetter\\ErrorLogger\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustBetter\\ErrorLogger\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
